{
    "kind": "BuildConfig",
    "apiVersion": "v1",
    "metadata": {
        "name": "s2i-httpd-sample-build",
    },
    "spec": {
        "triggers": [
            {
                "type": "ImageChange",
            }
        ],
        "source": {
            "type": "Git",
            "git": {
                "uri": "git@github.com:ajsmith/s2i-httpd-sample.git"
            }
        },
        "strategy": {
            "type": "Source",
            "sourceStrategy": {
                "from": {
                    "kind": "ImageStreamTag",
                    "name": "rhel6:latest"
                }
            }
        },
        "output": {
            "to": {
                "kind": "ImageStreamTag",
                "name": "s2i-httpd-sample:latest"
            }
        }
    }
}
